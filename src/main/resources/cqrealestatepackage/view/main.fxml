<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="mainBorderPane" prefHeight="700.0" prefWidth="1100.0" style="-fx-background-color: E6F7E9;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cqrealestatepackage.controller.MainController">
   <left>
      <HBox BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <VBox prefHeight="600.0" prefWidth="320.0" styleClass="body" stylesheets="@../styles/styles.css">
               <children>
                  <HBox alignment="CENTER" prefHeight="150.0" prefWidth="350.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CQ Real Estate">
                           <font>
                              <Font size="36.0" />
                           </font>
                        </Text>
                     </children>
                  </HBox>
                  <Button fx:id="btnDashboard" focusTraversable="false" mnemonicParsing="false" onAction="#gotoDashboard" prefHeight="80.0" prefWidth="350.0" styleClass="btn" text="Dashboard">
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnMakeASale" focusTraversable="false" layoutX="10.0" layoutY="128.0" mnemonicParsing="false" onAction="#gotoMakeASale" prefHeight="80.0" prefWidth="350.0" styleClass="btn" text="Make A Sale">
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnAddBuyer" focusTraversable="false" layoutX="10.0" layoutY="208.0" mnemonicParsing="false" onAction="#gotoAddBuyer" prefHeight="80.0" prefWidth="350.0" styleClass="btn" text="Add Buyer">
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnAddSeller" focusTraversable="false" layoutX="10.0" layoutY="304.0" mnemonicParsing="false" onAction="#gotoAddSeller" prefHeight="80.0" prefWidth="350.0" styleClass="btn" text="Add Seller">
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnAddProperty" focusTraversable="false" layoutX="10.0" layoutY="392.0" mnemonicParsing="false" onAction="#gotoAddProperty" prefHeight="80.0" prefWidth="350.0" styleClass="btn" text="Add Property">
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin>
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </HBox.margin>
            </VBox>
            <Pane prefHeight="700.0" prefWidth="20.0" HBox.hgrow="ALWAYS" />
            <Separator orientation="VERTICAL" prefHeight="678.0" prefWidth="13.0" />
            <Pane prefHeight="700.0" prefWidth="20.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </left>
</BorderPane>
